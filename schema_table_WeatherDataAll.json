{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "WeatherData",
    "type": "object",
    "properties": {
        "city": {
            "type": "string",
            "maxLength": 255
        },
        "temperature": {
            "type": "number"
        },
        "humidity": {
            "type": "integer"
        },
        "wind_speed": {
            "type": "number"
        },
        "weather_description": {
            "type": "string",
            "maxLength": 255
        },
        "pressure": {
            "type": "number"
        },
        "visibility": {
            "type": "integer"
        },
        "sunrise": {
            "type": "integer"
        },
        "sunset": {
            "type": "integer"
        },
        "country": {
            "type": "string",
            "maxLength": 255
        },
        "cloudiness": {
            "type": "integer"
        },
        "coord_lon": {
            "type": "number"
        },
        "coord_lat": {
            "type": "number"
        },
        "weather_id": {
            "type": "integer"
        },
        "weather_main": {
            "type": "string",
            "maxLength": 255
        },
        "timezone": {
            "type": "integer"
        },
        "id": {
            "type": "integer"
        }
    },
    "required": [
        "city",
        "temperature",
        "humidity",
        "wind_speed",
        "weather_description",
        "pressure",
        "visibility",
        "sunrise",
        "sunset",
        "country",
        "cloudiness",
        "coord_lon",
        "coord_lat",
        "weather_id",
        "weather_main",
        "timezone",
        "id"
    ]
}